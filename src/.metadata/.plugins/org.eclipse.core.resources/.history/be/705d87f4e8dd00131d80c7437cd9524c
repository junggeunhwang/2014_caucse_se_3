

import java.util.Vector;

public class TreeNode {
	
	
	public String key;
	public Vector child;

	void TreeNode(String keyName)
	{
		this.key = keyName;
		this.child = new Vector();
	}
	
	void TreeNode(String[] moduleOrder)
	{
		for(int i=0; i<moduleOrder.length; i++)
		{
			TreeNode T;
			T.TreeNode(moduleOrder[i]);
			child.add(moduleOrder[i]);
		}
	}
	
	void insertNode(TreeNode T, String[] members)
	{
		int insertPoint=0;
		for(int i=child.size()-1; i>=0; i--)  // group을 inset하고자 하는 root노드에서 그룹멤버들을 child node에서 제거한다.
		{
			for(int j=0; j<members.length; j++)
				if(child.get(i)==members[j])
				{
					child.remove(i);
					insertPoint = i;
				}
		}
		child.insertElementAt(T.key,insertPoint);
		T.TreeNode(members);
	}
	
	void removeNode(TreeNode T)
	{
		if()
	}
	
	
}

